#ulibdns pkg-config source file

prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: ulibdns
Cflags: -I${includedir}
Libs: -L${libdir} -l:libulibdns.@SHARED_LIBRARY_EXTENSION@.@MAJORVER@.@MINORVER@

Description: universal DNS library
Version: @VERSION@
Requires: ulib


